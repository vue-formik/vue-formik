{
  "name": "vue-formik",
  "version": "0.0.1",
  "keywords": [
    "vue",
    "formik",
    "vue3",
    "vue3-formik",
    "vue-formik",
    "formik-vue",
    "vue-formik-vue",
    "vue-formik-form",
    "vue-formik-forms",
    "vue-formik-formik"
  ],
  "description": "Vue 3 Formik",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/vue-formik-index.cjs.js",
  "module": "dist/vue-formik-index.es.js",
  "types": "dist/vue-formik-index.d.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-formik-index.es.js",
      "require": "./dist/vue-formik-index.cjs.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vue-formik/vue-formik"
  },
  "bugs": {
    "url": "https://github.com/vue-formik/vue-formik/issues",
    "email": "kiranparajuli589@gmail.com"
  },
  "author": {
    "name": "Kiran Parajuli",
    "email": "kiranparajuli589@gmail.com"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/eslint-plugin": "1.1.20",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.14.0",
    "eslint-plugin-playwright": "^2.1.0",
    "eslint-plugin-vue": "^9.30.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.3.3",
    "typescript": "~5.6.3",
    "vite": "^6.0.5",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10",
    "yup": "^1.6.1"
  },
  "license": "LGPL-3.0-or-later"
}
