{
  "name": "vue-formik",
  "version": "0.2.0",
  "keywords": [
    "vue",
    "formik",
    "vue3",
    "vue3-formik",
    "vue-formik",
    "formik-vue",
    "vue-form",
    "vue-form-validation"
  ],
  "description": "Form management library for Vue.js inspired by Formik - Simple, flexible, and performant form validation and state management",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/vue-formik.cjs.js",
  "module": "dist/vue-formik.es.js",
  "browser": "dist/vue-formik.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-formik.es.js",
      "require": "./dist/vue-formik.cjs.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit:ci": "vitest --run --no-coverage",
    "test:coverage": "vitest --run --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --check lib/ tests/",
    "format:fix": "prettier --write lib/ tests/",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vue-formik/vue-formik"
  },
  "bugs": {
    "url": "https://github.com/vue-formik/vue-formik/issues",
    "email": "kiranparajuli589@gmail.com"
  },
  "author": {
    "name": "Kiran Parajuli",
    "email": "kiranparajuli589@gmail.com"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": false
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/node": "^24.9.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vitest/coverage-istanbul": "4.0.6",
    "@vitest/eslint-plugin": "1.4.0",
    "@vitest/ui": "^4.0.6",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.0",
    "eslint": "^9.39.0",
    "eslint-plugin-vue": "^10.5.1",
    "husky": "^9.1.7",
    "joi": "^18.0.1",
    "jsdom": "^27.1.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.93.3",
    "superstruct": "^2.0.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.0",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vitest": "^4.0.6",
    "vue-tsc": "^3.1.2",
    "yup": "^1.7.1",
    "zod": "^4.1.0",
    "vue": "^3.5.13",
    "lodash.clonedeep": "^4.5.0"
  },
  "license": "LGPL-3.0-or-later",
  "pre-commit": {
    "silent": false,
    "run": [
      "lint:fix",
      "format:fix"
    ]
  }
}
