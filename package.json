{
  "name": "vue-formik",
  "version": "0.1.36",
  "keywords": [
    "vue",
    "formik",
    "vue3",
    "vue3-formik",
    "vue-formik",
    "formik-vue",
    "vue-form",
    "vue-form-validation"
  ],
  "description": "Vue 3 Formik",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/vue-formik.cjs.js",
  "module": "dist/vue-formik.es.js",
  "browser": "dist/vue-formik.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-formik.es.js",
      "require": "./dist/vue-formik.cjs.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit:ci": "vitest --run --no-coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --check lib/ tests/",
    "format:fix": "prettier --write lib/ tests/",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vue-formik/vue-formik"
  },
  "bugs": {
    "url": "https://github.com/vue-formik/vue-formik/issues",
    "email": "kiranparajuli589@gmail.com"
  },
  "author": {
    "name": "Kiran Parajuli",
    "email": "kiranparajuli589@gmail.com"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/coverage-istanbul": "2.1.8",
    "@vitest/eslint-plugin": "1.1.20",
    "@vitest/ui": "^2.1.8",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "husky": "^9.1.7",
    "joi": "^17.13.3",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.3.3",
    "sass-embedded": "^1.83.0",
    "typescript": "~5.6.3",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.4.0",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10",
    "yup": "^1.6.1",
    "zod": "^3.24.1"
  },
  "license": "LGPL-3.0-or-later",
  "pre-commit": {
    "silent": false,
    "run": [
      "lint:fix",
      "format:fix"
    ]
  }
}
